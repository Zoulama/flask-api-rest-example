
{
  "user_schema": {
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The User Schema",
  "required": [
    "email",
    "organizationId",
    "accountId"
  ],
  "properties": {
    "lastName": {
      "$id": "#/properties/lastName",
      "type": "string",
      "title": "User Last Name Schema",
      "default": "",
      "examples": [
        "John"
      ],
      "pattern": "^(.*)$"
    },
    "firstName": {
      "$id": "#/properties/firstName",
      "type": "string",
      "title": "User FirstName Schema",
      "default": "",
      "examples": [
        "Doe"
      ],
      "pattern": "^(.*)$"
    },
    "address": {
      "$id": "#/properties/address",
      "type": "object",
      "title": "User Address Schema",
      "required": [
        "streetName",
        "streetNumber",
        "city",
        "postCode",
        "state",
        "country"
      ],
      "properties": {
        "streetName": {
          "$id": "#/properties/address/properties/streetName",
          "type": "string",
          "title": "User Address Street Name Schema",
          "default": "",
          "examples": [
            "Astra House"
          ],
          "pattern": "^(.*)$"
        },
        "streetNumber": {
          "$id": "#/properties/address/properties/streetNumber",
          "type": "string",
          "title": "User Address Street Number Schema",
          "default": "",
          "examples": [
            "201"
          ]
        },
        "city": {
          "$id": "#/properties/address/properties/city",
          "type": "string",
          "title": "User Address City Schema",
          "default": "",
          "examples": [
            "New Cross"
          ],
          "pattern": "^(.*)$"
        },
        "postCode": {
          "$id": "#/properties/address/properties/postCode",
          "type": "string",
          "title": "The Postcode Schema",
          "default": "",
          "examples": [
            "SE14 24"
          ],
          "pattern": "^(.*)$"
        },
        "state": {
          "$id": "#/properties/address/properties/state",
          "type": "string",
          "title": "User Address State Schema",
          "default": "",
          "examples": [
            "London"
          ],
          "pattern": "^(.*)$"
        },
        "country": {
          "$id": "#/properties/address/properties/country",
          "type": "string",
          "title": "User Address Country Schema",
          "default": "",
          "examples": [
            "United Kingdom"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "User Email Schema",
      "default": "",
      "examples": [
        "my-email@gmail.com"
      ],
      "pattern": "^(.*)$"
    },
    "phoneNumber": {
      "$id": "#/properties/phoneNumber",
      "type": "string",
      "title": "User Phone number Schema",
      "default": "",
      "examples": [
        "00447307568957"
      ],
      "pattern": "^(.*)$"
    },
    "mobileNumber": {
      "$id": "#/properties/mobileNumber",
      "type": "string",
      "title": "User Mobile number Schema",
      "default": "",
      "examples": [
        "00447307568957"
      ],
      "pattern": "^(.*)$"
    },
    "language": {
      "$id": "#/properties/language",
      "type": "string",
      "title": "User Language Schema",
      "default": "",
      "examples": [
        "en"
      ],
      "pattern": "^(.*)$"
    },
    "organizationId": {
      "$id": "#/properties/organizationId",
      "type": "string",
      "title": "organizationId Schema",
      "default": "",
        "pattern": "^(.*)$"
      },
    "accountId": {
      "$id": "#/properties/organizationId",
      "type": "string",
      "title": "organizationId Schema",
      "default": "",
        "pattern": "^(.*)$"
      }
    }
  }
}

